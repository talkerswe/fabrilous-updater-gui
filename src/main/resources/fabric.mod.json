{
  "schemaVersion": 1,
  "id": "fabrilousupdatergui",
  "version": "${version}",
  "name": "Fabrilous Updater GUI",
  "description": "Easy mod updater for fabric.",
  "authors": [
    "HughBone",
    "TheEnderOfficial",
    "pozitp"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/fabrilous-updater-gui",
    "issues": "https://github.com/pozitp/fabrilous-updater-gui/issues",
    "sources": "https://github.com/pozitp/fabrilous-updater-gui"
  },
  "license": "MIT",
  "icon": "assets/fabrilousupdatergui/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.hughbone.fabrilousupdater.FabrilousUpdater"
    ],
    "modmenu": [
      "com.github.pozitp.config.ModMenuConfig"
    ]
  },
  "mixins": [
    "fabrilousupdatergui.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.12",
    "modmenu": "~${modmenu_version}",
    "fabric-language-kotlin": ">=${fabric_kotlin_version}",
    "minecraft": "1.17.x",
    "java": ">=16"
  }
}
